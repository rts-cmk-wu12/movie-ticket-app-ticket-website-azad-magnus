{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~components/*": [
                "src/style/components/*"
            ],
            "~pages/*": [
                "src/pages/*"
            ],
            "~style/*": [
                "src/style/*"
            ],
            "~utils/*": [
                "src/utils/*"
            ],
            "~/*": [
                "src/*"
            ],
            "~svg/*": [
                "src/assets/svg/*"
            ]
        }
    }
}